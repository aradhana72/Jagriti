                </div>
                <div class="form-field">
                    <div class="icon"> <i class="fa fa-envelope" aria-hidden="true"></i> </div>
                  <input type="email" id="inputEmail" name="email" class="form-input" placeholder="Email address">
                </div>
                 <!-- <button class="w-100 btn btn-lg btn-primary" type="submit" style="font-family: 'Open Sans';">Sign in</button> -->
               <input class="submit-button" type="submit" name="submit" value="SIGN UP" style="font-family: 'Open Sans';">

